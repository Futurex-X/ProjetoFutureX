    }                                        

    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {                                         
                                         
    // Cria a nova tela
    JogoDaVelha jogo = new JogoDaVelha();
    // Torna visível
    jogo.setVisible(true);
    // Opcional: fecha a janela atual, se desejar
    this.dispose();


    }                                        

